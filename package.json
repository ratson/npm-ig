{
  "name": "npm-ig",
  "version": "0.0.0",
  "description": "Install global dependencies from package.json",
  "bin": "lib/cli.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "rimraf lib",
    "prepublish": "run-s clean build",
    "test": "eslint --ext js --ext md ."
  },
  "dependencies": {
    "cross-spawn-promise": "^0.9.1",
    "lodash": "^4.17.4",
    "promise-exit": "^0.0.2",
    "read-pkg": "^2.0.0",
    "update-notifier": "^1.0.3",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.5",
    "babel-register": "^6.18.0",
    "chalk": "^1.1.3",
    "cross-env": "^3.1.4",
    "eslint": "^3.12.2",
    "eslint-config-concise": "^0.6.1",
    "eslint-config-concise-esnext": "^0.6.0",
    "npm-run-all": "^4.0.0",
    "rimraf": "^2.5.4"
  },
  "preferGlobal": true,
  "repository": "ratson/npm-ig",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "keywords": [
    "npm",
    "install",
    "global",
    "dependencies",
    "package",
    "packages",
    "module",
    "command",
    "cmd",
    "cli",
    "bin"
  ]
}
